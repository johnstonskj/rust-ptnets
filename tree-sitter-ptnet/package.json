{
  "name": "tree-sitter-ptnet",
  "version": "0.0.1",
  "description": "ptnet grammar for tree-sitter",
  "author": "Simon Johnston <johnstonskj@gmail.com>",
  "license": "Apache-2.0",
  "main": "bindings/node",
  "keywords": [
    "parser",
    "net"
  ],
  "dependencies": {
    "nan": "^2.12.1"
  },
  "devDependencies": {
    "tree-sitter-cli": "^0.20.8"
  },
  "scripts": {
    "test": "tree-sitter test"
  },
  "directories": {
    "test": "test"
  },
  "tree-sitter": [
    {
      "scope": "source.ptnet",
      "file-types": [
        "ptnet", "ptn"
      ],
      "folds": [
        "queries/folds.scm"
      ],
      "highlights": [
        "queries/highlights.scm"
      ],
      "indent": [
        "queries/indent.scm"
      ],
      "injections": [
        "queries/injections.scm"
      ],
      "locals": [
        "queries/locals.scm"
      ],
      "tags": [
        "queries/tags.scm"
      ],
      "injection-regex": "^ptnet"
    }
  ]
}
